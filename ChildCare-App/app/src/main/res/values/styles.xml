<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="what_s_your">
        <item name="android:textSize">40sp</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="calendar_button">
        <item name="android:layout_width">4dp</item>
        <item name="android:layout_height">4dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:padding">2dp</item>
        <item name="android:background">@color/fillColor</item>
        <item name="android:textOn">"n"</item>
        <item name="android:textOff">"f"</item>
    </style>
    <style name="tap_the_tim">

        <item name="android:textSize">
            20sp
        </item>

        <item name="android:textColor">
            #000000
        </item>

    </style>


</resources>
